{
    "name": "YunoHost example app",
    "id": "ynhexample",
    "packaging_format": 1,
    "description": {
        "en": "Example package for YunoHost application.",
        "fr": "Exemple de package dâ€™application pour YunoHost."
    },
    "version": "1.0",
    "url": "https://example.com",
    "license": "free",
    "maintainer": {
        "name": "John doe",
        "email": "john.doe@example.com",
        "url": "http://example.com"
    },
    "requirements": {
        "yunohost": ">= 2.7.2"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "ask": {
                    "en": "Choose a domain name for ynhexample",
                    "fr": "Choisissez un nom de domaine pour ynhexample"
                },
                "example": "example.com"
            },
            {
                "name": "room_domain",
                "ask": {
                    "en": "Choose a domain name for ynhexample",
                    "fr": "Choisissez un nom de domaine pour ynhexample"
                },
                "example": "conference.domain.tld",
                "default": "conference.yunohost.org"
            }
        ]
    }
}
